# Streamlit Secrets Configuration
# For Streamlit Cloud deployment, add these secrets in your app dashboard
# For local development, copy this file to .streamlit/secrets.toml

# OpenAI Configuration
OPENAI_API_KEY = "your-openai-api-key-here"
OPENAI_MODEL = "gpt-4"  # or gpt-3.5-turbo for faster processing
OPENAI_EMBEDDING_MODEL = "text-embedding-3-small"

# Pinecone Configuration
PINECONE_API_KEY = "your-pinecone-api-key-here"
PINECONE_ENVIRONMENT = "us-east-1-aws"  # Your Pinecone environment
PINECONE_INDEX = "flash-report-index"  # Your Pinecone index name 